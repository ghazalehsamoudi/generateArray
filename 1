#include<iostream>
#include<conio.h>
using namespace std;
void print(int[],int);
main(){
	const int arraysize=6;
	int num[arraysize];
	generateArray(num,arraysize);
	print(num,arraysize);
}
void generateArray(int a[],int sizeOfarray){
	for(int i=0;i<sizeOfarray;i++)
	     cin>>a[i];
}
void print(int a[],int sizeOfarry){
	for(int i=0;i<sizeOfarry;i++)
	      cout<<a[i]<<'\t';
}
